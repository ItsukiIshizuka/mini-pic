<div class="contents row">
  <%= form_for(@tweet) do |f| %>
    <h3>
      投稿する
    </h3>
    <%= f.text_field :image, name: "image", multiple: true, placeholder: 'Image Url' %>
    <%# <input placeholder="Image Url" type="text" name="image"> %>
    <%= f.text_area :text, name: "text", placeholder: 'Text' %>
    <%= f.submit %>
  <% end %>
</div>