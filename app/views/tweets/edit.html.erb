<div class="contents row">
  <%= form_for @tweet, url: {action: 'update'} do |f| %>
    <h3>
      編集する
    </h3>
    <%= f.text_field :image, name: "image", placeholder: 'Image Url' %>
    <%= f.text_field :image2, name: "image2", placeholder: 'Image Url2' %>
    <%= f.text_field :image3, name: "image3", placeholder: 'Image Url3' %>
    <%= f.text_area :text, name: "text", placeholder: 'Text' %>
    <%= f.submit %>
  <% end %>
  </div>